{
    "type": "control-sender",
    "publicKeyFile": "${workspaceRoot}/test-data/rsa-keys/sender.public",
    "privateKeyFile": "${workspaceRoot}/test-data/rsa-keys/sender.private",
    "receiverRsaKeyFolder": "${workspaceRoot}/test-data/rsa-keys/",
    "receiverConfigFile": "${workspaceRoot}/test-data/control-crypto-shovel/receivers.json",
    "from": {
        "connectionUrl": "amqp://open_amqp",
        "binding": {
            "exchange": "control-src-exchange",
            "exchangeType": "fanout",
            "options": {
                "autoDelete": true
            }
        }
    },
    "to": {
        "connectionUrl": "amqp://open_amqp",
        "binding": {
            "exchange": "control-encrypted-exchange",
            "exchangeType": "fanout",
            "options": {
                "autoDelete": true
            }
        }
    }
}