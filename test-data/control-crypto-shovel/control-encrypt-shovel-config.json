{
    "type": "control-sender",
    "publicRsaKeyFile": "${workspaceRoot}/test-data/rsa-keys/sender.public",
    "privateRsaKeyFile": "${workspaceRoot}/test-data/rsa-keys/sender.private",
    "keyReceiverRsaKeyFolder": "${workspaceRoot}/test-data/rsa-keys/",
    "keyReceiverConfigFile": "${workspaceRoot}/test-data/control-crypto-shovel/keyReceivers.json",
    "from": {
        "connectionUrl": "amqp://open_amqp",
        "binding": {
            "exchange": "control-src-exchange",
            "exchangeType": "fanout",
            "options": {
                "autoDelete": true
            }
        }
    },
    "to": {
        "connectionUrl": "amqp://open_amqp",
        "binding": {
            "exchange": "control-encrypted-exchange",
            "exchangeType": "fanout",
            "options": {
                "autoDelete": true
            }
        }
    }
}